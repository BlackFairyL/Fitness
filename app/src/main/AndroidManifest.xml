<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.fitnes">
<uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/dumbbell"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/dumbbell"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".TrainingChoosing"
            android:label="Выбор типа тренировки" />
        <activity
            android:name=".TrainingDescription"
            android:label="Описание тренировки" />
        <activity
            android:name=".ExerciseChoosing"
            android:label="Выберите упражнения" />
        <activity
            android:name=".ExerciseDescription"
            android:label="Описание упражнения" />
    </application>

</manifest>